<?xml version="1.0" encoding="UTF-8"?>
<!ATTLIST telephone
	type (professionnel | personnel | portable) #IMPLIED
>


<!ELEMENT bibliotheque (ouvrage| livre)*>

<!ELEMENT ouvrage (type, nom, IBN, location)>
<!ELEMENT location (meuble, etagère)>
<!ELEMENT livre (nom, serie, edition, auteur, position)>


<!ELEMENT type (#PCDATA)>
<!ELEMENT nom (#PCDATA)>
<!ELEMENT IBN (#PCDATA)>
<!ELEMENT meuble (#PCDATA)>
<!ELEMENT etagère (#PCDATA)>


<!ELEMENT serie (#PCDATA)>
<!ELEMENT auteur (#PCDATA)>
<!ELEMENT position (#PCDATA)>